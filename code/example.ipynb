   "source": [
    "# Given values\n",
    "I = 1600  # Initial investment\n",
    "C = 200   # Annual cash flow\n",
    "r = 0.1   # Discount rate\n",
    "\n",
    "# Sum of the infinite geometric series\n",
    "S = C / (1 - (1 / (1 + r)))\n",
    "print(S)\n",
    "# Calculating NPV\n",
    "NPV = -I + S\n",
    "\n",
    "NPV"
   ]
